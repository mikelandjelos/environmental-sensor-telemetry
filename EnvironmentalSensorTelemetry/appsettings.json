{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http2"
    }
  },
  "InfluxDB": {
    "ConfigFilePath": "./InfluxDB/Config/influx-configs",
    "DefaultOrganization": "IoTSOrg",
    "DefaultBucket": "EnvironmentalSensorTelemetry"
  }
}
